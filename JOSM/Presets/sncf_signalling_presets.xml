<?xml version="1.0" encoding="UTF-8"?>
<presets xmlns="http://josm.openstreetmap.de/tagging-preset-1.0"
    author="Noël Danjou"
    version="1.[[revision]]_[[date]]"
    description="Presets for tagging French railway signalling"
    fr.description="Préréglages pour étiqueter la signalisation ferroviaire en France"
    shortdescription="SNCF Signalling"
    fr.shortdescription="Signalisation SNCF">

	<chunk id="common_tags">
		<key key="railway" value="signal" />
		<key key="source" value="SNCF" />
		<key key="source:date" value="2022-03" />
		<text key="ref" text="Identifiant" />
		<text key="railway:position:exact" text="PK (ex. : 100.325)" />
		<combo key="railway:signal:direction" text="Sens" values="forward,backward,both" display_values="Croissant,Décroissant,Bidirectionnel" default="forward" editable="false" />
		<combo key="railway:signal:position" text="Position" values="left,right,bridge,overhead,in_track" display_values="Gauche,Droite,Portique/Potence,Caténaire,Heurtoir" default="left" editable="false" />
	</chunk>

	<chunk id="main_signal_tags">
		<reference ref="common_tags" />
		<key key="railway:signal:main:form" value="light" />
	</chunk>

	<chunk id="distant_signal_tags">
		<reference ref="common_tags" />
		<key key="railway:signal:distant:form" value="light" />
		<key key="railway:signal:distant:shape" value="FR:R" />
	</chunk>

	<group name="Signalisation SNCF">

		<!-- Carré -->
		<chunk id="carre_options_tags">
			<optional>
				<text key="railway:signal:main:caption:marker" text="Intitulé du Repérage" default="" />
				<combo key="railway:signal:main:plate:block" text="Plaque de Cantonnement" default="" editable="false">
					<list_entry value="FR:BM" display_value="BM" short_description="Block Manuel" />
					<list_entry value="FR:PR" display_value="PR" short_description="Block Automatique à Permissivité Restreinte" />
					<list_entry value="" display_value="-- Aucune --" />
				</combo>
				<text key="railway:signal:main:caption:block" text="Intitulé du Cantonnement" default="" />
			</optional>
		</chunk>

		<chunk id="carre_tags">
			<reference ref="main_signal_tags" />
			<key key="railway:signal:main" value="FR:CARRE" />
			<key key="railway:signal:main:plate" value="FR:NF" />

			<item_separator />

			<checkgroup columns="3">
				<check key="railway:signal:main:clearing_light" text="Œilleton" default="on" />
				<check key="railway:signal:main:short_route" text="Bande Jaune" default="off" />
				<check key="railway:signal:main:height" text="Taille Réduite" value_on="dwarf" value_off="normal" default="off" />
			</checkgroup>

			<reference ref="carre_options_tags" />
			<link wiki="OpenRailwayMap/Tagging_in_France#Carré" />
		</chunk>


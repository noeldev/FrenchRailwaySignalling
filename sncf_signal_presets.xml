                <key key="railway:signal:main:type" value="FR:F1" />
                <key key="railway:signal:main:states" 
                    value="FR:C;FR:S;FR:(S);FR:M;FR:(M);FR:A;FR:(A);FR:R;FR:R+A;FR:R+(A);FR:(R);FR:(R)+A;FR:(R)+(A);FR:VL;FR:(VL)" />
            </item>

            <item name="Cible F3 (6 feux)" type="node">
                <label text="Carré sur cible type &quot;F3&quot;" /><space />
                <reference ref="carre_tags" />

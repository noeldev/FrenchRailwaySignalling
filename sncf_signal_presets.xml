                <key key="railway:signal:route_distant" value="FR:TIDD" />
                <key key="railway:signal:route_distant:form" value="light" />
                <link wiki="OpenRailwayMap/Tagging in France#Tableau Indicateur de Direction à Distance" />
            </item>

        </group>

        <group name="Tableaux Indicateurs de Vitesse">

            <group name="TIV Mobiles">

                <chunk id="mobile_speed_indicator_tags">
                    <reference ref="common_tags" />
                    <link wiki="OpenRailwayMap/Tagging in France#Mobile Speed Indicators" />
                </chunk>

                <item name="TIV Mobile à Distance" type="node">
                    <label text="TIV Mobile à Distance" /><space />
                    <reference ref="mobile_speed_indicator_tags" />
                    <key key="railway:signal:speed_limit_distant" value="FR:TIV-D_MOB" />
                    <text key="railway:signal:speed_limit_distant:speed" text="Vitesse limite" default="60" />
                    <checkgroup>
                        <check key="railway:signal:speed_limit_distant:form" text="Éclairé" value_off="sign" value_on="light" default="on" />
                        <check key="railway:crocodile" text="Crocodile" default="yes" />

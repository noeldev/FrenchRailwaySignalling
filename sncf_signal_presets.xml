                <key key="railway:signal:main:type" value="FR:C3" />
                <key key="railway:signal:main:states" 
                    value="FR:C;FR:S;FR:(S);FR:A;FR:(A);FR:VL;FR:(VL)" />
            </item>

            <separator />

            <item name="Cible F1 (7 feux)" type="node">
                <label text="Carré sur cible type &quot;F1&quot;" /><space />

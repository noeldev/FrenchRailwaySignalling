                <key key="railway:signal:main:type" value="FR:F3" />
                <key key="railway:signal:main:states" 
                    value="FR:C;FR:S;FR:(S);FR:A;FR:(A);FR:R;FR:R+A;FR:R+(A);FR:(R);FR:(R)+A;FR:(R)+(A);FR:VL;FR:(VL)" />
            </item>

            <separator />

            <item name="Cible H1 (9 feux)" type="node">
                <label text="Carré sur cible type &quot;H1&quot;" /><space />

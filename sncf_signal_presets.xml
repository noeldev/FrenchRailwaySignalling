            <reference ref="distant_signal_tags" />
            <key key="railway:signal:distant" value="FR:D" />
            <key key="railway:signal:distant:shape" value="FR:R" />
            <link wiki="OpenRailwayMap/Tagging in France#Disque" />
        </chunk>

        <group name="Disque">

            <item name="Cible R3 (3 feux)" type="node">
                <label text="Disque sur cible type &quot;R3&quot;" /><space />
                <reference ref="disk_tags" />
                <key key="railway:signal:distant:type" value="FR:R3" />
                <key key="railway:signal:distant:states" 

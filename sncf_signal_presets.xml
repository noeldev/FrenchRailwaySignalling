            </item>

            <item name="Cible H4 (7 feux)" type="node">
                <label text="Carré sur cible type &quot;H4&quot;" /><space />
                <reference ref="carre_tags" />
                <key key="railway:signal:main:shape" value="FR:H" />
                <key key="railway:signal:main:type" value="FR:H4" />
                <key key="railway:signal:main:states" 
                    value="FR:C;FR:S;FR:(S);FR:M;FR:(M);FR:RR;FR:RR+A;FR:RR+(A);FR:(RR);FR:(RR)+A;FR:(RR)+(A);FR:A;FR:(A);FR:VL;FR:(VL)" />
            </item>

            <item name="Cible H6 (6 feux)" type="node">
                <label text="Carré sur cible type &quot;H6&quot;" /><space />
                <reference ref="carre_tags" />
                <key key="railway:signal:main:shape" value="FR:H" />
                <key key="railway:signal:main:type" value="FR:H6" />
                <key key="railway:signal:main:states" 
                    value="FR:C;FR:S;FR:(S);FR:RR;FR:RR+A;FR:RR+(A);FR:(RR);FR:(RR)+A;FR:(RR)+(A);FR:A;FR:(A);FR:VL;FR:(VL)" />
            </item>

        </group>

        <!-- Carré violet -->
        <chunk id="cv_tags">
            <reference ref="main_signal_tags" />
            <key key="railway:signal:main" value="FR:CV" />
            <key key="railway:signal:main:plate" value="FR:NF" />
            <check key="railway:signal:main:height" text="Sur mât" value_off="low" value_on="high" default="on" editable="false" />
            <reference ref="carre_options_tags" />
            <link wiki="OpenRailwayMap/Tagging in France#Carré violet" />
        </chunk>

        <group name="Carré violet">

            <item name="Cible A2 (2 feux)" type="node">
                <label text="Carré violet sur cible type &quot;A2&quot;" /><space />
                <reference ref="cv_tags" />
                <key key="railway:signal:main:shape" value="FR:A" />
                <key key="railway:signal:main:type" value="FR:A2" />
                <key key="railway:signal:main:states" 
                    value="FR:CV;FR:M;FR:(M)" />
            </item>

            <separator />

            <item name="Cible C2 (5 feux)" type="node">
                <label text="Carré violet sur cible type &quot;C2&quot;" /><space />
                <reference ref="cv_tags" />
                <key key="railway:signal:main:shape" value="FR:C" />
                <key key="railway:signal:main:type" value="FR:C2" />
                <key key="railway:signal:main:states" 

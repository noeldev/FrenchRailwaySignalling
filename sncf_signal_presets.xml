            </item>

            <item name="Cible H7 (6 feux)" type="node">
                <label text="Carré violet sur cible type &quot;H7&quot;" /><space />
                <reference ref="cv_tags" />
                <key key="railway:signal:main:shape" value="FR:H" />
                <key key="railway:signal:main:type" value="FR:H7" />
                <key key="railway:signal:main:states" 
                    value="FR:CV;FR:S;FR:(S);FR:RR;FR:RR+A;FR:RR+(A);FR:(RR);FR:(RR)+A;FR:(RR)+(A);FR:A;FR:(A);FR:VL;FR:(VL)" />
            </item>

            <separator />

            <item name="Cible K2 (2 feux)" type="node">
                <label text="Carré violet sur cible type &quot;K2&quot;" /><space />
                <reference ref="cv_tags" />
                <key key="railway:signal:main:shape" value="FR:K" />
                <key key="railway:signal:main:type" value="FR:K2" />
                <key key="railway:signal:main:states" value="FR:CV;FR:M;FR:(M)" />
            </item>

        </group>

        <!-- Guidon d'Arrêt -->
        <item name="Guidon d'Arrêt" type="node">
            <label text="Guidon d'Arrêt" /><space />
            <reference ref="main_signal_tags" />
            <key key="railway:signal:main" value="FR:GA" />
            <optional>
                <text key="railway:signal:main:caption:marker" text="Intitulé de repérage" />
            </optional>
            <link wiki="OpenRailwayMap/Tagging in France#Guidon d'Arrêt" />
        </item>

        <separator />

        <!-- Sémaphore -->
        <chunk id="semaphore_tags">
            <reference ref="main_signal_tags" />
            <key key="railway:signal:main" value="FR:S"  />
            <combo key="railway:signal:main:plate" text="Plaque d'Identification" values="FR:F,FR:BM,FR:PR" display_values="F,BM,PR" default="FR:F" editable="false" />
            <link wiki="OpenRailwayMap/Tagging in France#Sémaphore" />
        </chunk>

        <group name="Sémaphore">

            <item name="Cible A1 (3 feux)" type="node">
                <label text="Sémaphore sur cible type &quot;A1&quot;" /><space />
                <reference ref="semaphore_tags" />
                <key key="railway:signal:main:shape" value="FR:A" />
                <key key="railway:signal:main:type" value="FR:A1" />
                <key key="railway:signal:main:states" 
                    value="FR:S;FR:(S);FR:A;FR:(A);FR:VL;FR:(VL)" />
            </item>

            <item name="Cible A3 (2 feux)" type="node">
                <label text="Sémaphore sur cible type &quot;A3&quot;" /><space />
                <reference ref="semaphore_tags" />
                <key key="railway:signal:main:shape" value="FR:A" />
                <key key="railway:signal:main:type" value="FR:A3" />
                <key key="railway:signal:main:states" 

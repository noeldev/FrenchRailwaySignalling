                <key key="railway:signal:main:states" 
                    value="FR:C;FR:S;FR:(S);FR:M;FR:(M);FR:RR;FR:RR+A;FR:RR+(A);FR:(RR);FR:(RR)+A;FR:(RR)+(A);FR:A;FR:(A);FR:R;FR:R+A;FR:R+(A);FR:(R);FR:(R)+A;FR:(R)+(A);FR:VL;FR:(VL)" />
            </item>

            <item name="Cible H3 (8 feux)" type="node">
